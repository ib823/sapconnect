# SAP Connect Environment Variables
# Copy this file to .env and fill in values for your client

# SAP System Connection (for live API discovery)
SAP_HOSTNAME=
SAP_CLIENT=100
SAP_USERNAME=
SAP_PASSWORD=

# AI Agent (for live AI-powered ABAP development)
AI_API_KEY=

# vsp (vibing-steampunk) - ADT bridge for live SAP access
VSP_PATH=
VSP_SYSTEM=

# OData Client - base URL for live OData connectivity
SAP_BASE_URL=
SAP_ODATA_VERSION=v2

# OAuth2 (alternative to basic auth - takes precedence if SAP_TOKEN_URL is set)
SAP_TOKEN_URL=
SAP_CLIENT_ID=
SAP_CLIENT_SECRET=
SAP_SCOPE=

# SAP RFC Connection (for live agent SAP access)
SAP_SYSNR=00
SAP_ROUTER=

# BTP Connection (for deployment)
BTP_SUBDOMAIN=
BTP_API_ENDPOINT=

# Application Settings
NODE_ENV=development
CDS_ENV=development
PORT=4004
