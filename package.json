{
  "name": "sapconnect",
  "version": "1.0.0",
  "description": "SAP Connect - Demo & Pitch Toolkit for SAP Public Cloud External Development",
  "repository": "https://github.com/your-org/sapconnect",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^8",
    "express": "^4",
    "@sap/xssec": "^4",
    "passport": "^0.7"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "@sap/ux-specification": "latest"
  },
  "scripts": {
    "start": "cds-serve",
    "watch": "cds watch",
    "demo": "cds watch --profile demo",
    "discover": "node discovery/discover.js",
    "agent": "node agent/agent.js",
    "assess": "node migration/assess.js",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "impl": "@cap-js/sqlite",
        "credentials": {
          "database": ":memory:"
        }
      },
      "API_BUSINESS_PARTNER": {
        "kind": "odata-v2",
        "model": "srv/external/API_BUSINESS_PARTNER",
        "[production]": {
          "credentials": {
            "destination": "S4HANA",
            "path": "/sap/opu/odata/sap/API_BUSINESS_PARTNER"
          }
        },
        "[development]": {
          "kind": "mocked"
        }
      }
    }
  }
}
