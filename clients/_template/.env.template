# Client Environment Configuration
# Copy this file to .env in the project root and fill in values

# Client Identification
CLIENT_NAME=
CLIENT_CODE=

# SAP System Connection
SAP_HOSTNAME=
SAP_CLIENT=100
SAP_USERNAME=
SAP_PASSWORD=

# SAP System Type: public-cloud | private-cloud | btp-only
SAP_SYSTEM_TYPE=public-cloud

# BTP Subaccount (for deployment)
BTP_SUBDOMAIN=
BTP_API_ENDPOINT=https://api.cf.us10.hana.ondemand.com

# AI Agent
ANTHROPIC_API_KEY=
ENABLE_AI_AGENT=false

# vsp (vibing-steampunk) - ADT bridge for live SAP access
VSP_PATH=
VSP_SYSTEM=

# Feature Flags
ENABLE_LIVE_APIS=false
ENABLE_EVENT_MESH=false
